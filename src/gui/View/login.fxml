<AnchorPane prefHeight="400.0" prefWidth="600.0" styleClass="anchorPane" stylesheets="@Style/buttonLoginStyle.css" xmlns="http://javafx.com/javafx/21" xmlns:fx="http://javafx.com/fxml/1" fx:controller="gui.Controlers.conForLogin">
   <children>
      <Button fx:id="loginButton" layoutX="258.0" layoutY="269.0" mnemonicParsing="false" onAction="#login" styleClass="buttonLogin" text="Login">
         <font>
            <Font size="15.0" />
         </font></Button>
      <Label layoutX="117.0" layoutY="134.0" styleClass="Label" text="Username:">
         <font>
            <Font size="20.0" />
         </font>
      </Label>
      <Label layoutX="119.0" layoutY="187.0" styleClass="Label" text="Password:">
         <font>
            <Font size="20.0" />
         </font>
      </Label>
      <TextField fx:id="nameTextField" styleClass="text-field" layoutX="262.0" layoutY="135.0" />
      <TextField fx:id="passwordTextField" styleClass="text-field" layoutX="262.0" layoutY="188.0" />
      <Label fx:id="LabelError" layoutX="271.0" layoutY="233.0" styleClass="Label">
         <font>
            <Font size="15.0" />
         </font>
      </Label>
      <RadioButton fx:id="rButton" layoutX="408.0" layoutY="275.0" mnemonicParsing="false" onAction="#radioRegister" styleClass="radioButton" text="Registration" />
      <Label fx:id="rReg" layoutX="299.0" layoutY="91.0" styleClass="Label" />
      <Button fx:id="butReg" layoutX="258.0" layoutY="269.0" mnemonicParsing="false" onAction="#registretion" styleClass="buttonLogin" stylesheets="@Style/buttonLoginStyle.css" text="Register">
         <font>
            <Font name="Apple Braille Outline 6 Dot" />
         </font></Button>
      <RadioButton fx:id="rExpert" layoutX="452.0" layoutY="169.0" mnemonicParsing="false" onAction="#radioExpert" styleClass="radioButton" text="Expert" />
   </children>
</AnchorPane>
