      <AnchorPane prefHeight="219.0" prefWidth="272.0" styleClass="anchorPaneVoting">
         <children>
            <Label fx:id="passwordLabel" layoutX="21.0" layoutY="118.0" styleClass="Label" text="Your Password:">
               <font>
                  <Font size="20.0" />
